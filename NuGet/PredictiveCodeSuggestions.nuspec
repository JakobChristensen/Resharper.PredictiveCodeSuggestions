<?xml version="1.0"?>
<package >
  <metadata>
    <id>PredictiveCodeSuggestions</id>
    <version>1.1.0</version>
    <title>Predictive Code Suggestions</title>
    <authors>Jakob Christensen</authors>
    <description>Suggests the next line of code by finding patterns in existing code. In many cases a line of code is often followed by a specific other line of code forming a common pattern of lines. Patent Pending.</description>
    <copyright>Copyright &#x00A9; Jakob Christensen</copyright>
    <projectUrl>https://github.com/JakobChristensen/Resharper.PredictiveCodeSuggestions</projectUrl>
    <tags>Code Completion</tags>
    <dependencies>
      <dependency id="ReSharper" version="8.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="v8.0\*.*" target="ReSharper\v8.0\plugins\PredictiveCodeSuggestions" />
    <file src="v8.1\*.*" target="ReSharper\v8.1\plugins\PredictiveCodeSuggestions" />
    <file src="v8.2\*.*" target="ReSharper\v8.2\plugins\PredictiveCodeSuggestions" />
    <file src="PredictiveCodeSuggestions.DotSettings" target="ReSharper\v8.2\settings\PredictiveCodeSuggestions" />
  </files>
</package>